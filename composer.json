  "type": "symfony-bundle",
  "description": "Symfony2 Bundle that adds basic user account entity and city/state/country setup as well",
  "keywords": [
    "KMJ",
    "toolkit"
  ],
  "homepage": "http://github.com/Demoboy/ToolkitBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Kaelin Jacobson",
      "email": "kaelinjacobson@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "zendframework/zend-crypt": "~2.6",
    "zendframework/zend-filter": "~2.6",
    "knplabs/knp-paginator-bundle": "~2.5",
    "friendsofsymfony/user-bundle": "^2.1",
    "doctrine/orm": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.2",
    "h4cc/alice-fixtures-bundle": "dev-master",
    "satooshi/php-coveralls": "~1.0"
  },
  "repositories": [
    {
