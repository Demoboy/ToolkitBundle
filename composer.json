{
    "name": "kmj/toolkitbundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle that adds basic user account entity and city/state/country setup as well",
    "keywords": ["KMJ", "toolkit"],
    "homepage": "http://github.com/Demoboy/ToolkitBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaelin Jacobson",
            "email": "kaelinjacobson@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/form": "2.6.*",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "doctrine/orm": "2.4.*",
        "misd/phone-number-bundle": "1.0.*",
        "symfony/validator": "2.*",
        "twig/twig": "1.*",
        "symfony/doctrine-bridge": "2.*",
        "symfony/expression-language": "2.*"
    },
    "require-dev": {
        "h4cc/alice-fixtures-bundle": "0.5.x",
        "matthiasnoback/symfony-config-test": "0.4.*",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-0": {
            "KMJ\\ToolkitBundle": ""
        }
    },
    "minimum-stability": "stable",
    "target-dir": "KMJ/ToolkitBundle"
}
